@model Data.Model.Entities.Dish

@{
    ViewBag.Title = "Edit";
    ViewBag.Current = "Dishes";
}
<div class="col-lg-6 col-xs-12">
    <div class="box-content card white2">
        <h4 class="box-title">Sửa nhóm món ăn</h4>
        <div class="card-content">
            @using (Html.BeginForm("Edit", "Dishes", null, FormMethod.Post,
                                          new { enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()

                <div>
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.ID)

                    @{ Html.RenderPartial("_Form", Model); }

                </div>
            }
        </div>
    </div>
</div>